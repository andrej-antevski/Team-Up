@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Team Up</h1>
    <p class="lead">Team Up is a platform for developers to find and create their groups for projects. Rreach out and communicate with developers just like you! Have an amazing idea for an app but can't find the right people to work with? Simply create or join your team and start coding!</p>
    <!--<p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p> --->
</div>

<div class="row">
    <div class="col-md-6">
        <h2>Log In</h2>
        <p>
            Already a user? Sweet! Simply log in here.
        </p>
        <!--<p><a class="btn btn-primary" href="https://go.microsoft.com/fwlink/?LinkId=301865">Log in &raquo;</a></p>-->
        <p>@Html.ActionLink("Log In", "Login", "Account", null, new { @class = "btn btn-primary" })</p>
    </div>
    <div class="col-md-6">
        <h2>Register</h2>
        <p>Not a user? Not a problem!</p>
        <!--<p><a class="btn btn-primary" href="https://go.microsoft.com/fwlink/?LinkId=301866">Register here &raquo;</a></p>-->
        <p>@Html.ActionLink("Register", "Register", "Account", null, new { @class = "btn btn-primary" })</p>
    </div>
    
</div>